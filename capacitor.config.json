{
  "appId": "com.breslovacademy.app",
  "appName": "Breslov Academy",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "launchFadeOutDuration": 200,
      "backgroundColor": "#ffffff",
      "androidSplashResourceName": "splash",
      "showSpinner": true,
      "spinnerColor": "#999999"
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    }
  }
}
